doctype 
html(lang="es")
    head 
        meta(charset="UTF-8")
        tittle For loop
        link(rel="stylesheet", href="../css/master.css")
    body 
        nav.controls 
            a(href="../index.html")
        main 
            header 
                h1 For loop
            section 
                ul 
                    -for(let i=1; 1<=20; i++)
                        li Element #{i}
                table(border=1)
                    -for(let tr=1; tr<=5; tr++)
                        tr 
                            -for(let td=1; td<=5; td++)
                                td F:#{tr} C:#{td}